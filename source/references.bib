@article{zhangEffectColorCoding2022,
  title = {The Effect of Color Coding and Layout Coding on Users‚Äô Visual Search on Mobile Map Navigation Icons},
  author = {Zhang, Mengzhe and Gong, Yong and Deng, Rong and Zhang, Sanyuan},
  date = {2022-12-13},
  journaltitle = {Frontiers in Psychology},
  shortjournal = {Front. Psychol.},
  volume = {13},
  publisher = {Frontiers},
  issn = {1664-1078},
